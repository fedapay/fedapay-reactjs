{
  "name": "fedapay-reactjs",
  "version": "1.0.0",
  "description": "FedaPay integration for ReactJs applications",
  "author": "FedaPay",
  "license": "MIT",
  "repository": "https://github.com/fedapay/fedapay-reactjs",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
},
  
  "scripts": {
    "build": "rollup -c && npm run copy",
    "start": "rollup -c -w",
    "copy": "node src/tools/copy-package.js",
    "prepare": "npm run build",
    "link-lib": "npm link && cd example && npm link fedapay-reactjs",
    "install-example": "cd example && npm install",
    "start-example": "cd example && npm run start"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@svgr/rollup": "^8.1.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^7.0.3",
    "gh-pages": "^6.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.21.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-url": "^3.0.1",
    "typescript": "^5.6.2"
  },
  "files": [
    "dist"
  ]
}
