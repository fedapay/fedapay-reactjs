{
  "name": "fedapay-reactjs",
  "version": "1.0.0",
  "description": "FedaPay integration for ReactJs applications",
  "author": "FedaPay",
  "license": "MIT",
  "repository": "https://github.com/fedapay/fedapay-reactjs",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "rollup -c && npm run copy",
    "start": "rollup -c -w",
    "copy": "node src/tools/copy-package.js",
    "prepare": "npm run build",
    "link-lib": "npm link && cd example && npm link fedapay-reactjs",
    "install-example": "cd example && npm install",
    "start-example": "cd example && npm run start"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "node-sass": "^4.12.0"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^17.0.0",
    "react-dom": "^15.0.0 || ^17.0.0"
  },
  "devDependencies": {
    "@svgr/rollup": "^2.4.1",
    "@types/react": "^16.3.13 || ^17.0.0",
    "@types/react-dom": "^16.0.5 || ^17.0.9",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "gh-pages": "^1.2.0",
    "react": "^16.4.1 || ^17.0.0",
    "react-dom": "^16.4.1 || ^17.0.0",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-typescript2": "^0.17.0",
    "rollup-plugin-url": "^1.4.0",
    "typescript": "^2.8.3"
  },
  "files": [
    "dist"
  ]
}
